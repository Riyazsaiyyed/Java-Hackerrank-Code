import java.io.*; 
import java.util.*; 
 
public class Solution { 
 
    public static void main(String[] args) { 
        Scanner sc = new Scanner(System.in); 
        String k = sc.nextLine(); 
        int n = k.length(); 
        int num = Integer.parseInt(k, 2); 
        if (num >= 65 && num <= 90) { 
            System.out.println((char) num); 
            System.out.println( 
                    "\n\nExplanation 0\n" + k + " represents " + num + " which corresponds to '" + (char) num + "'."); 
        } 
        else 
        { 
            System.out.println("INVALID PAPER CODE"); 
            System.out.println("\nExplanation 1\n" + k + " is having inly " + n + " bits"); 
        } 
    } 
 
}
