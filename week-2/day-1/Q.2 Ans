import java.io.*; 
import java.util.*; 
 
public class Solution { 
 
    public static void main(String[] args) {  
        Scanner sc = new Scanner(System.in); 
        int n = sc.nextInt(); 
        int x = sc.nextInt(); 
        int sum = n + x; 
        System.out.println(sum); 
         
    } 
}
