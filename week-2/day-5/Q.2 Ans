import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        double num = sc.nextDouble();
        int num1 = (int)num;
        if(num >= 0)
            System.out.println(num1);
        else
            System.out.println("Invalid Input");
        
    }
}
