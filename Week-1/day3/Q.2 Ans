import java.io.*; 
import java.util.*; 
 
public class Solution { 
 
    public static void main(String[] args) { 
        Scanner sc = new Scanner(System.in); 
        int n = sc.nextInt(); 
        int i = 1; 
        while(i*i*i < n){ 
            i++; 
        } 
        if(i*i*i == n){ 
            System.out.println("Perfect Cube"); 
        }else{ 
            System.out.println("Not Perfect Cube"); 
        
        } 
    } 
}
