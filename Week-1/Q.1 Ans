import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int w=sc.nextInt();
        int x=sc.nextInt();
        if((w>=0)&&(x>=0))
        {
            System.out.println(w+"-"+x);
        }
        else
        {
            System.out.println("Invalid Input");
        }
    }
}
